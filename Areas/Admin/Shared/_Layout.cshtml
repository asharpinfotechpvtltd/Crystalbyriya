<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Astaberry Admin</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="images/favicon.png">
    <!-- Custom Stylesheet -->
    <link href="~/Admin/css/style.css" rel="stylesheet">

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
        Nav header start
    ***********************************-->
        <div class="nav-header">
            <div class="brand-logo">
                <a href="index.html">
                    <b class="logo-abbr"><img src="~/Areas/Admin/images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/Areas/Admin/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="~/Areas/Admin/images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
        <!--**********************************
        Nav header end
    ***********************************-->
        <!--**********************************
        Header start
    ***********************************-->
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
               
                
            </div>
        </div>
        <!--**********************************
        Header end ti-comment-alt
    ***********************************-->
        <!--**********************************
        Sidebar start
    ***********************************-->
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label">Dashboard</li>
                    <li>
                        <a class="has-arrow" href="/Admin/index" aria-expanded="false">
                            <i class="icon-speedometer menu-icon"></i><span class="nav-text">HOME</span>
                        </a>

                    </li>
                    <li>
                        <a asp-page="/Orders/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">ORDERS</span>
                        </a>
                    </li>
                    <li>
                        <a asp-page="/Banners/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">BANNERS</span>
                        </a>
                    </li>  
                    <li>
                        <a asp-page="/OtherBanners/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">OTHER BANNERS</span>
                        </a>
                    </li>
              
                    <li>
                        <a asp-page="/Concerns/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">CONCERNS</span>
                        </a>
                    </li>
                    <li>
                        <a asp-page="/CouponCodes/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">COUPON CODES</span>
                        </a>
                    </li>
                    <li>
                        <a asp-page="/ImageGalleries/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">IMAGE GALLERIES</span>
                        </a>
                    </li>
                  
                    <li>
                        <a asp-page="/ProductDetail/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">PRODUCTS DETAIL</span>
                        </a>
                    </li>
                    
                  
                    <li>
                        <a asp-page="/Reviews/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">REVIEWS</span>
                        </a>
                    </li>
                  
                  
                    <li>
                        <a asp-page="/Blog/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">BLOG</span>
                        </a>
                    </li>
                    
                    <li>
                        <a asp-page="/JobPosting/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">JOB</span>
                        </a>
                    </li>
                    <li>
                        <a asp-page="/Reports/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">REPORT</span>
                        </a>
                    </li>
                    <li>
                        <a asp-page="/OfferImages/Index" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">OFFERIMAGES</span>
                        </a>
                    </li>
                    <li class="mega-menu mega-menu-sm">
                        <a class="has-arrow" href="javascript:void()" aria-expanded="false">
                            <i class="icon-globe-alt menu-icon"></i><span class="nav-text">PROMOTION</span>
                        </a>
                        <ul aria-expanded="false">
                            <li><a asp-page="/Advertise/SendSms">SMS</a></li>
                            <li><a asp-page="/Advertise/Create">EMAIL</a></li>
                          
                          
                        </ul>
                    </li>
                    <li>
                        <a asp-page="/Index" asp-page-handler="Logout" aria-expanded="false">
                            <i class="icon-lock menu-icon"></i><span class="nav-text">SIGNOUT</span>
                        </a>
                    </li>

                </ul>
            </div>
        </div>
        <!--**********************************
        Sidebar end
    ***********************************-->
        <!--**********************************
        Content body start
    ***********************************-->
        <div class="content-body">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="form-validation">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--**********************************
            Content body end
        ***********************************-->
            <!--**********************************
            Footer start
        ***********************************-->
            <div class="footer">
                <div class="copyright">
                    <p>Copyright &copy; Designed & Developed by <a href="https://themeforest.net/user/quixlab">Quixlab</a> 2018</p>
                </div>
            </div>
            <!--**********************************
            Footer end
        ***********************************-->
        </div>
        <!--**********************************
        Main wrapper end

    ***********************************-->
    </div>
            <!--**********************************
            Scripts
        ***********************************-->

            <script src="~/Admin/plugins/common/common.min.js"></script>
            <script src="~/Admin/js/custom.min.js"></script>
            <script src="~/Admin/js/settings.js"></script>
            <script src="~/Admin/js/gleek.js"></script>
            <script src="~/Admin/js/styleSwitcher.js"></script>
            @RenderSection("scripts", required: false)
</body>

</html>